{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "removeComments": true, 
        "strict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "moduleResolution": "node"
    }
}